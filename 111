
    static JFrame frame = new JFrame("test");

    public static void main(String[] args) {
        frame.setLayout(new GridBagLayout());
        frame.setVisible(true);
        JPanel left = new JPanel(new GridBagLayout());
        JPanel right = new JPanel(new GridBagLayout());
        JPanel rightTop = new JPanel();
        rightTop.setLayout(new GridBagLayout());
        rightTop.setBorder(BorderFactory.createTitledBorder("Path 1"));
        JPanel rightDown = new JPanel(new GridBagLayout());
        rightDown.setBorder(BorderFactory.createTitledBorder("Analysis"));
        JPanel leftTop = new JPanel();
        leftTop.setLayout(new BoxLayout(leftTop, BoxLayout.Y_AXIS));
        leftTop.setBorder(BorderFactory.createTitledBorder("Path Selection"));
        JPanel leftDown = new JPanel();
        leftDown.setLayout(new BoxLayout(leftDown, BoxLayout.Y_AXIS));
        leftDown.setBorder(BorderFactory.createTitledBorder("Electronics"));
        JLabel leftTopLeftLabel = new JLabel("    Path");
        JLabel leftTopRightLabel = new JLabel("inv");
        JPanel leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(leftTopLeftLabel);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(leftTopRightLabel);
        leftTop.add(leftTopTmp);
        ButtonGroup l1Group = new ButtonGroup();
        JRadioButton l1 = new JRadioButton("L1");
        l1.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l1Inv = new JRadioButton();
        l1Group.add(l1);
        l1Group.add(l1Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l1);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l1Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup l2Group = new ButtonGroup();
        JRadioButton l2 = new JRadioButton("L2");
        l2.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l2Inv = new JRadioButton();
        l2Group.add(l2);
        l2Group.add(l2Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l2);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l2Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup l3Group = new ButtonGroup();
        JRadioButton l3 = new JRadioButton("L3");
        l3.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l3Inv = new JRadioButton();
        l3Group.add(l3);
        l3Group.add(l3Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l3);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l3Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup l4Group = new ButtonGroup();
        JRadioButton l4 = new JRadioButton("L4");
        l4.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l4Inv = new JRadioButton();
        l4Group.add(l4);
        l4Group.add(l4Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l4);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l4Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup l5Group = new ButtonGroup();
        JRadioButton l5 = new JRadioButton("L5");
        l5.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l5Inv = new JRadioButton();
        l5Group.add(l5);
        l5Group.add(l5Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l5);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l5Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup l6Group = new ButtonGroup();
        JRadioButton l6 = new JRadioButton("L6");
        l6.setHorizontalTextPosition(SwingConstants.LEFT);
        JRadioButton l6Inv = new JRadioButton();
        l6Group.add(l6);
        l6Group.add(l6Inv);
        leftTopTmp = new JPanel();
        leftTopTmp.setLayout(new BoxLayout(leftTopTmp, BoxLayout.X_AXIS));
        leftTopTmp.add(l6);
        leftTopTmp.add(Box.createRigidArea(new Dimension(10,0)));
        leftTopTmp.add(l6Inv);
        leftTop.add(leftTopTmp);
        ButtonGroup leftDownGroup = new ButtonGroup();
        JRadioButton leftDownTop = new JRadioButton("Series IV");
        leftDownTop.setAlignmentX(Component.CENTER_ALIGNMENT);
        JRadioButton leftDownDown = new JRadioButton("Series VI");
        leftDownDown.setAlignmentX(Component.CENTER_ALIGNMENT);
        leftDownGroup.add(leftDownTop);
        leftDownGroup.add(leftDownDown);
        leftDown.add(leftDownTop);
        leftDown.add(leftDownDown);
        add(left,leftTop,0,0,0.7,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(left,leftDown,1,0,0.3,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        JPanelCat.jump(panel -> {
            GridBagConstraints c0 = new GridBagConstraints();
            c0.gridx = 0;
            c0.gridy = 0;
            c0.gridwidth = 1;
            c0.gridheight = 1;
            c0.weightx = 0.5;
            c0.weighty = 1;
            c0.fill = GridBagConstraints.WEST;
            c0.anchor = GridBagConstraints.BOTH;
            panel.add(rightTop,c0);
            return panel;
        });
        add(right,rightTop,0,0,0.5,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(right,rightDown,1,0,0.5,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(frame,left,0,0,1,0.3);
        add(frame,right,0,1,1,0.7);
        add(rightTop,new Label("No. of  Pulses:"),0,0,0,0,1,2,GridBagConstraints.WEST,GridBagConstraints.NONE);
        JTextField jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(4);
        add(rightTop,jTextField,0,2,0,0,1,1,GridBagConstraints.WEST,GridBagConstraints.NONE);
        add(rightTop,new Label("Pulse:"),0,3,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH,new Insets(0,10,0,0));
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(3);
        add(rightTop,jTextField,0,4,0,0,GridBagConstraints.WEST,GridBagConstraints.NONE);
        JButton jButton = new JButton("Prev");
