        jButton = new JButton("Fix Axis");
        add(rightTop,jButton,0,7,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH,new Insets(0,10,0,0));
        add(rightTop,new Label("Invalid"),1,1,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(rightTop,new Label("Error"),1,2,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(rightTop,new JButton("Free Axis"),1,7,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH,new Insets(10,10,0,0));
        add(rightTop,new Label("Trd A:"),2,0,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(4);
        add(rightTop,jTextField,2,1,0,0,GridBagConstraints.WEST,GridBagConstraints.NONE);
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(32);
        add(rightTop,jTextField,2,2,0,0,1,5,GridBagConstraints.WEST,GridBagConstraints.NONE);
        add(rightTop,new Label("Trd B:"),3,0,0,0,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(4);
        add(rightTop,jTextField,3,1,0,0,GridBagConstraints.WEST,GridBagConstraints.NONE);
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(32);
        add(rightTop,jTextField,3,2,0,0,1,5,GridBagConstraints.WEST,GridBagConstraints.NONE);
        JPanel rightDownLeft = new JPanel();
        rightDownLeft.setLayout(new BoxLayout(rightDownLeft, BoxLayout.Y_AXIS));
        JPanel rightDownRight = new JPanel(new GridBagLayout());
        JButton avgButton = new JButton("Avg Pulse");
        JLabel treshold = new JLabel("Treshold (%):");
        jTextField = new JTextField();
        jTextField.setEditable(true);
        jTextField.setColumns(4);
        rightDownLeft.add(Box.createVerticalStrut(30));
        avgButton.setAlignmentX(Component.CENTER_ALIGNMENT);
        rightDownLeft.add(avgButton);
        JPanel tmp = new JPanel(new FlowLayout());
        tmp.add(treshold);
        tmp.add(jTextField);
        rightDownLeft.add(Box.createVerticalStrut(10));
        rightDownLeft.add(tmp);
        add(rightDown,rightDownLeft,0,0,1,0.3,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        FlowLayout flowLayout = new FlowLayout();
        flowLayout.setHgap(10);
        JPanel rightDownRightTop = new JPanel(flowLayout);
        rightDownRightTop.setBorder(BorderFactory.createTitledBorder("Pulse Zero"));
        JPanel rightDownRightDown = new JPanel(flowLayout);
        rightDownRightDown.setBorder(BorderFactory.createTitledBorder("Transducer Zero"));
        JButton prevButton = new JButton("Prev");
        JButton nextButton = new JButton("Next");
        JButton invalidButton = new JButton("Invalid");
        rightDownRightTop.add(prevButton);
        rightDownRightTop.add(nextButton);
        rightDownRightTop.add(invalidButton);
        rightDownRightDown.add(new JButton("Prev"));
        rightDownRightDown.add(new JButton("Next"));
        add(rightDownRight,rightDownRightTop,0,0,0.5,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(rightDownRight,rightDownRightDown,1,0,0.5,1,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        add(rightDown,rightDownRight,0,1,1,0.7,GridBagConstraints.WEST,GridBagConstraints.BOTH);
        frame.pack();
        frame.setLocationRelativeTo(null);
        frame.setVisible(true);
        left.setPreferredSize(new Dimension(200,600));
        right.setPreferredSize(new Dimension(600,600));
    }

    public static void add(JComponent master,Component child,int y, int x,double weighty,double weightx,int anchor,int fill){
        GridBagConstraints c0 = new GridBagConstraints();
        c0.gridx = x;
        c0.gridy = y;
        c0.gridwidth = 1;
        c0.gridheight = 1;
        c0.weightx = weightx;
        c0.weighty = weighty;
        c0.fill = fill;
        c0.anchor = anchor;
        master.add(child, c0);
    }
    public static void add(JComponent master,Component child,int y, int x,double weighty,double weightx,int gridheight,int gridwidtx,int anchor,int fill){
        GridBagConstraints c0 = new GridBagConstraints();
        c0.gridx = x;
        c0.gridy = y;
        c0.gridwidth = gridwidtx;
        c0.gridheight = gridheight;
        c0.weightx = weightx;
        c0.weighty = weighty;
        c0.fill = fill;
        c0.anchor = anchor;
        master.add(child, c0);
    }


    public static void add(JComponent master,Component child,int y, int x,double weighty,double weightx,int anchor,int fill,Insets insets){
        GridBagConstraints c0 = new GridBagConstraints();
        c0.gridx = x;
        c0.gridy = y;
        c0.gridwidth = 1;
        c0.gridheight = 1;
        c0.weightx = weightx;
        c0.weighty = weighty;
        c0.fill = fill;
        c0.insets = insets;
        c0.anchor = anchor;
        master.add(child, c0);
    }

    public static void add(JFrame master,JComponent child,int y,int x,double weighty,double weightx){
        GridBagConstraints c0 = new GridBagConstraints();
        c0.gridx = x;
        c0.gridy = y;
        c0.gridwidth = 1;
        c0.gridheight = 1;
        c0.weightx = weightx;
        c0.weighty = weighty;
        c0.fill = GridBagConstraints.BOTH;
        c0.anchor = GridBagConstraints.WEST;
        master.add(child, c0);
    }
